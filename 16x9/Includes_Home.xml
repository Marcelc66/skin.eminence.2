                </control>
            </control>
            <control type="button" id="$PARAM[buttonid]">
                <animation effect="slide" start="0" end="4" condition="true">Conditional</animation>
                <left>-$PARAM[width]</left>
                <top>16</top>
                <width>$PARAM[width]</width>
                <height>170</height>
                <texturenofocus colordiffuse="HomeBarTile" border="50">common/tile.png</texturenofocus>
                <texturefocus colordiffuse="$VAR[HighlightColor]" border="50">common/tile.png</texturefocus>
                <aspectratio>stretch</aspectratio>
                <textcolor>00ffffff</textcolor>
                <focusedcolor>00ffffff</focusedcolor>
                <onclick condition="$PARAM[set-onclick-target]">SetProperty(FO9001,2,home)</onclick>
                <onclick condition="$PARAM[set-onclick-target]">SetProperty(WindowBaseReturn,$PARAM[onclick-target],home)</onclick>
                <onclick condition="$PARAM[wickedxxx]">SetProperty(PASSWORD,1,home)</onclick>
                <onclick condition="$PARAM[wickedxxx]">Skin.Reset(ADULTPASS)</onclick>
                <onclick condition="$PARAM[wickedxxx]">Skin.SetNumeric(ADULTPASS)</onclick>
                <onclick condition="$PARAM[wickedxxx]">ActivateWindow(1175)</onclick>
                <onclick>$PARAM[onclick]</onclick>
                <onfocus>$PARAM[onfocus]</onfocus>
                <onfocus>$PARAM[onfocus2]</onfocus>
                <onunfocus>SetProperty(ButtonSwitch,1,Home)</onunfocus>
                <onunfocus>AlarmClock(ButtonSwitchOff,SetProperty(ButtonSwitch,0,Home),00:01,silent)</onunfocus>
                <font />
                <label />
                <visible>$PARAM[visible]</visible>
            </control>
            <control type="group" id="$PARAM[groupid]">
                <top>16</top>
                <left>-$PARAM[width]</left>
                <width>$PARAM[width]</width>
                <height>170</height>
                <visible>$PARAM[visible]</visible>
                <control type="group">
                    <animation effect="fade" start="100" end="50" condition="!Control.HasFocus($PARAM[groupid]) + !Control.HasFocus($PARAM[buttonid])">Conditional</animation>
                    <control type="image">
                        <top>-32</top>
                        <left>16</left>
                        <texture colordiffuse="HomeBarFG" diffuse="common/tile.png" aligny="center" align="center">$PARAM[icon]</texture>
                        <aspectratio scalediffuse="false">center</aspectratio>
                    </control>
                    <control type="label">
                        <top>104</top>
                        <height>78</height>
                        <left>16</left>
                        <right>20</right>
                        <align>center</align>
                        <aligny>center</aligny>
                        <font>Font-HomeMenu</font>
                        <textcolor>HomeBarFG</textcolor>
                        <label>$PARAM[label]</label>
                    </control>
                </control>
            </control>
        </definition>
    </include>



